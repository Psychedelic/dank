{
  "version": 1,
  "dfx": "0.7.1",
  "canisters": {
    "dank": {
      "build": "node build.js",
      "candid": "candid/dank.did",
      "wasm": "target/wasm32-unknown-unknown/release/dank-opt.wasm",
      "type": "custom"
    },
    "piggy-bank": {
      "build": "cargo build --target wasm32-unknown-unknown --release --package piggy-bank",
      "candid": "candid/piggy-bank.did",
      "wasm": "target/wasm32-unknown-unknown/release/piggy_bank.wasm",
      "type": "custom"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  }
}
